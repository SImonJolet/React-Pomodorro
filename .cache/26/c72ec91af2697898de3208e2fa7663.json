terval);\n        };\n    }, [paused]);\n\n    const setPlus = () => {\n        setSeconde(seconde + 60);\n    };\n    const setMoins = () => {\n        setSeconde(seconde - 60);\n    };\n    const startTimer = () => {\n        setPaused(false);\n    };\n    const pauseTimer = () => {\n        setPaused(true);\n    };\n    const resetTimer = () => {\n        setPaused(true);\n        setSeconde(25 * 60);\n    };\n    return (\n        <div>\n            <div class={\"timer\"}>\n                {`${Math.floor(seconde / 60)\n                    .toString()\n                    .padStart(2, \"0\")}:${`${seconde % 60}`\n                    .toString()\n                    .padStart(2, \"0\")}`}\n            </div>\n            <div className=\"bouton\">\n                {\" \"}\n                <button id={\"plusBtn\"} type={\"button\"} onClick={setPlus}>\n                    {\"Plus\"}\n                </button>\n                <button\n                    id={\"startStopBtn\"}\n                    type={\"button\"}\n                    onClick={paused ? startTimer : pauseTimer}>\n                    {paused ? \"Start\" : \"Pause\"}\n                </button>\n                <button id={\"resetBtn\"} type={\"button\"} onClick={resetTimer}>\n                    {\"Reset\"}\n                </button>\n                <button id={\"moinsBtn\"} type={\"button\"} onClick={setMoins}>\n                    {\"Moins\"}\n                </button>\n            </div>\n        </div>\n    );\n};\n"},"lineCount":null}},"error":null,"hash":"9f6d01802eb616c6e03e192c18c1da9b","cacheData":{"env":{}}}